{
  "name": "mini-trading-terminal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html --only-categories=performance --view --chrome-flags='--headless'",
    "lighthouse:ci": "lighthouse http://localhost:4173 --output=json --output-path=./lighthouse-report.json --only-categories=performance --chrome-flags='--headless' --quiet",
    "report:lighthouse": "node scripts/lighthouse-report.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "tsc --noEmit && npm run format:check && npm run lint && npm run test"
  },
  "dependencies": {
    "@codex-data/sdk": "^1.0.6",
    "@radix-ui/react-dialog": "^1.1.11",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-virtual": "^3.13.19",
    "@types/recharts": "^2.0.1",
    "axios": "^1.13.6",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "decimal.js": "^10.6.0",
    "framer-motion": "^12.34.3",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.503.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.2.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.15.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bn.js": "^5.2.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.13.0",
    "jsdom": "^28.1.0",
    "lighthouse": "^13.0.3",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vite-plugin-compression2": "^2.4.0",
    "vitest": "^2.1.4",
    "zod": "^3.23.8"
  }
}
